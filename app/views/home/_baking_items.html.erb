<table class="striped">
  <thead>
    <tr>
      <th width = "500"><%= item_type.titleize %></th>
      <th>Quantity</th>
    </tr>
  </thead>
  <tbody>
    <% items.each do |item, quantity| %>
      <tr>
        <td><%= item %></td>
        <td><%= quantity %></td>
      </tr>
    <% end %>
  </tbody>
</table>
