<% unless @inactive_items.empty? || !logged_in? %>
  <div class="panel clearfix">
    <h3>Inactive Items</h3>
    <ul>
      <% @inactive_items.each do |item| %>
        <li><%= link_to item.name, item_path(item) %></li>
      <% end %>
    </ul>
  </div>
<% end %>

